<package>
	<id>com.brunonfl.wijoin</id>
	<name>WiJoin</name>
	<version>1.1-53</version>
	<compatibility>
		<firmware>
      <miniOS>7.0</miniOS>
			<maxiOS>9.3.3</maxiOS>
			<otherVersions>unconfirmed</otherVersions>
			<!--
			for otherVersions, you can put either unsupported or unconfirmed
			-->
		</firmware>
	</compatibility>
	<dependencies>
        <package>mobilesubstrate</package>
    </dependencies>
	<descriptionlist>
        <description>Compatible with iOS 7 to 9</description>
        <description><![CDATA[
					Simple tweak to display the Wi-Fi network you connected to like in Android.
					Short Hold the message to open to Wi-Fi settings!<br/><br/>
					Configure options in settings.
					]]>
		</description>
	</descriptionlist>
	<screenshots>
        <screenshot>
            <description>Tweak in action.</description>
            <image>1.PNG</image>
        </screenshot>
        <screenshot>
            <description>Tweak in action.</description>
            <image>2.PNG</image>
        </screenshot>
    </screenshots>
	<changelog>
    <change><![CDATA[
			<strong>v1.1-53</strong> <br/>
			- Fixed label not resizing for long Wi-Fi names.<br/>
			- Added option to show alert when disconnected.<br/>
			- Added the ability to show disconnected and connected alert at the same time.]]></change>
	</changelog>
	<links></links>
</package>
